@Article{ahmed1998transition,
  author    = {Ahmed, S. H. and Koob, G. F.},
  title     = {Transition from Moderate to Excessive Drug Intake: Change in Hedonic Set Point},
  journal   = {Science},
  year      = {1998},
  volume    = {282},
