@Article{maisto1979test,
  author   = {Maisto, Stephen A. and Sobell, Mark B. and Cooper, A. Mitch and Sobell, Linda C.},
  title    = {Test-retest reliability of retrospective self-reports in three populations of alcohol abusers},
  journal  = {Journal of Behavioral Assessment},
