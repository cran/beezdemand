@Article{jarmolowicz2012using,
  author   = {David P. Jarmolowicz and Warren K. Bickel and Anne E. Carter and Christopher T. Franck and E. Terry Mueller},
  title    = {Using crowdsourcing to examine relations between delay and probability discounting},
  journal  = {Behavioural Processes},
  year     = {2012},
  volume   = {91},
  number   = {3},
  pages    = {308--312},
  abstract = {Although the extensive lines of research on delay and/or probability discounting have greatly expanded our understanding of human decision-making processes, the relation between these two phenomena remains unclear. For example, some studies have reported robust associations between delay and probability discounting, whereas others have failed to demonstrate a consistent relation between the two. The current study sought to clarify this relation by examining the relation between delay and probability discounting in a large sample of internet users (n = 904) using the Amazon Mechanical Turk (AMT) crowdsourcing service. Because \{AMT\} is a novel data collection platform, the findings were validated through the replication of a number of previously established relations (e.g., relations between delay discounting and cigarette smoking status). A small but highly significant positive correlation between delay and probability discounting rates was obtained, and principal component analysis suggested that two (rather than one) components were preferable to account for the variance in both delay and probability discounting. Taken together, these findings suggest that delay and probability discounting may be related, but are not manifestations of a single construct (e.g., impulsivity). },
  doi      = {10.1016/j.beproc.2012.09.001},
  issn     = {0376-6357},
  keywords = {Delay discounting, Probability discounting, Crowdsourcing, Amazon Mechanical Turk, Monetary choice questionnaire, Human },
  url      = {http://www.sciencedirect.com/science/article/pii/S0376635712001891},
}

@Article{bickel2014using,
  author   = {Warren K. Bickel and A. George Wilson and Christopher T. Franck and E. Terry Mueller and David P. Jarmolowicz and Mikhail N. Koffarnus and Samantha J. Fede},
  title    = {Using crowdsourcing to compare temporal, social temporal, and probability discounting among obese and non-obese individuals},
  journal  = {Appetite},
  year     = {2014},
  volume   = {75},
  pages    = {82--89},
  abstract = {Abstract Previous research comparing obese and non-obese samples on the delayed discounting procedure has produced mixed results. The aim of the current study was to clarify these discrepant findings by comparing a variety of temporal discounting measures in a large sample of internet users (n = 1163) obtained from a crowdsourcing service, Amazon Mechanical Turk (AMT). Measures of temporal, social--temporal (a combination of standard and social temporal), and probability discounting were obtained. Significant differences were obtained on all discounting measures except probability discounting, but the obtained effect sizes were small. These data suggest that larger-N studies will be more likely to detect differences between obese and non-obese samples, and may afford the opportunity, in future studies, to decompose a large obese sample into different subgroups to examine the effect of other relevant measures, such as the reinforcing value of food, on discounting. },
  doi      = {10.1016/j.appet.2013.12.018},
  issn     = {0195-6663},
  keywords = {Obesity, Decision making, Temporal discounting, Social discounting, Probability discounting, Crowdsourcing },
  url      = {http://www.sciencedirect.com/science/article/pii/S0195666313005059},
}

