@Article{hursh1988cost,
  author   = {Hursh, Steven R. and Raslear, Thomas G. and Shurtleff, David and Bauman, Richard and Simmons, Laurence},
  title    = {A cost-benefit analysis of demand for food.},
  journal  = {Journal of the Experimental Analysis of Behavior},
  year     = {1988},
  volume   = {50},
  number   = {3},
  pages    = {419--440},
  abstract = {Laboratory studies of consumer demand theory require assumptions regarding the definition of price in the absence of a medium of exchange (money). In this study we test the proposition that the fundamental dimension of price is a cost-benefit ratio expressed as the effort expended per unit of food value consumed. Using rats as subjects, we tested the generality of this {\textquotedblleft}unit price{\textquotedblright} concept by varying four dimensions of price: fixed-ratio schedule, number of food pellets per fixed-ratio completion, probability of reinforcement, and response lever weight or effort. Two levels of the last three factors were combined in a 2 $\times$ 2 $\times$ 2 design giving eight groups. Each group was studied under a series of six FR schedules. Using the nominal values of all factors to determine unit price, we found that grams of food consumed plotted as a function of unit price followed a single demand curve. Similarly, total work output (responses $\times$ effort) conformed to a single function when plotted in terms of unit price. These observations provided a template for interpreting the effects of biological factors, such as brain lesions or drugs, that might alter the cost-benefit ratio.},
  doi      = {10.1901/jeab.1988.50-419},
  issn     = {1938-3711},
  keywords = {fixed-ratio schedules, closed economy, reinforcer size, reinforcer probability, lever weight, response cost, VMH lesions, lever press, rats},
  url      = {http://dx.doi.org/10.1901/jeab.1988.50-419},
}

